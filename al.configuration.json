{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["THEN BEGIN", "END"],
        ["THEN BEGIN", "END;"],
        ["THEN begin", "end"],
        ["THEN begin", "end;"],
        ["THEN Begin", "End"],
        ["THEN Begin", "End;"],
        ["then BEGIN", "END"],
        ["then BEGIN", "END;"],
        ["then begin", "end"],
        ["then begin", "end;"],
        ["then Begin", "End"],
        ["then Begin", "End;"],
        ["Then BEGIN", "END"],
        ["Then BEGIN", "END;"],
        ["Then begin", "end"],
        ["Then begin", "end;"],
        ["Then Begin", "End"],
        ["Then Begin", "End;"],
        ["BEGIN", "END;"],
        ["begin", "end;"],
        ["Begin", "End;"]
    ],
    "autoClosingPairs": [
        { "open": "THEN BEGIN", "close": "END", "notIn": ["string", "comment"] },
        { "open": "THEN BEGIN", "close": "END;", "notIn": ["string", "comment"] },
        { "open": "THEN begin", "close": "end", "notIn": ["string", "comment"] },
        { "open": "THEN begin", "close": "end;", "notIn": ["string", "comment"] },
        { "open": "THEN Begin", "close": "End", "notIn": ["string", "comment"] },
        { "open": "THEN Begin", "close": "End;", "notIn": ["string", "comment"] },
        { "open": "then BEGIN", "close": "END", "notIn": ["string", "comment"] },
        { "open": "then BEGIN", "close": "END;", "notIn": ["string", "comment"] },
        { "open": "then begin", "close": "end", "notIn": ["string", "comment"] },
        { "open": "then begin", "close": "end;", "notIn": ["string", "comment"] },
        { "open": "then Begin", "close": "End", "notIn": ["string", "comment"] },
        { "open": "then Begin", "close": "End;", "notIn": ["string", "comment"] },
        { "open": "Then BEGIN", "close": "END", "notIn": ["string", "comment"] },
        { "open": "Then BEGIN", "close": "END;", "notIn": ["string", "comment"] },
        { "open": "Then begin", "close": "end", "notIn": ["string", "comment"] },
        { "open": "Then begin", "close": "end;", "notIn": ["string", "comment"] },
        { "open": "Then Begin", "close": "End", "notIn": ["string", "comment"] },
        { "open": "Then Begin", "close": "End;", "notIn": ["string", "comment"] },
        { "open": "BEGIN", "close": "END;", "notIn": ["string", "comment"] },
        { "open": "begin", "close": "end;", "notIn": ["string", "comment"] },
        { "open": "Begin", "close": "End;", "notIn": ["string", "comment"] },
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
        { "open": "\"", "close": "\"", "notIn": ["string"] }
    ],
    "surroundingPairs": [
        ["'", "'"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
          "start": "^\\s*#?region\\b",
          "end": "^\\s*#?endregion\\b"
        }
    },
    "wordPattern": "(\"(?:(?:\\\"\\\")|[^\\\"])*\")|(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}